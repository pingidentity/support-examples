dn: cn=schema
objectClass: top
objectClass: ldapSubentry
objectClass: subschema
cn: schema
attributeTypes: ( lastLogoff-OID NAME 'lastLogoff' DESC 'This attribute is not used. (and I\27m quoting MS docs here people) It tracks each time a user logs off. But then again it\27s not used...' SINGLE-VALUE USAGE userApplications X-MAX-VALUE-LENGTH '8' )
attributeTypes: ( uSNChanged-OID NAME 'uSNChanged' DESC 'The update sequence number (USN) assigned by the local directory for the latest change, including creation.' SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 SINGLE-VALUE USAGE userApplications )
attributeTypes: ( countryCode-OID NAME 'countryCode' DESC 'Specifies the country/region code for the user\27s language of choice. This value is not used by Windows 2000.' SINGLE-VALUE USAGE userApplications X-MIN-VALUE-LENGTH '0' X-MAX-VALUE-LENGTH '2' )
attributeTypes: ( badPasswordTime-OID NAME 'badPasswordTime' DESC 'The last time and date that an attempt to log on to this account was made with a password that is not valid. This value is stored as a large integer that represents the number of 100-nanosecond intervals since January 1, 1601 (UTC). A value of zero means that the last time a incorrect password was used is unknown.' SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 SINGLE-VALUE USAGE userApplications )
attributeTypes: ( whenChanged-OID NAME 'whenChanged' DESC 'The date when this object was last changed. This value is not replicated and exists in the global catalog.' SYNTAX 1.3.6.1.4.1.1466.115.121.1.24 SINGLE-VALUE USAGE userApplications )
attributeTypes: ( company-OID NAME 'company' DESC 'The user\27s company name.' SINGLE-VALUE USAGE userApplications )
attributeTypes: ( objectGUID-OID NAME 'objectGUID' DESC 'The unique identifier for an object.The AD equivalent of entryUUID' SYNTAX 1.3.6.1.4.1.1466.115.121.1.40 SINGLE-VALUE USAGE userApplications )
attributeTypes: ( pwdLastSet-OID NAME 'pwdLastSet' DESC 'The date and time that the password for this account was last changed. This value is stored as a large integer that represents the number of 100 nanosecond intervals since January 1, 1601 (UTC). If this value is set to 0 and the User-Account-Control attribute does not contain the UF_DONT_EXPIRE_PASSWD flag, then the user must set the password at the next logon.' SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 SINGLE-VALUE USAGE userApplications )
attributeTypes: ( badPwdCount-OID NAME 'badPwdCount' DESC 'The number of times the user tried to log on to the account using an incorrect password. A value of 0 indicates that the value is unknown.' SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 SINGLE-VALUE USAGE userApplications X-MIN-INT-VALUE '0' X-MAX-INT-VALUE '9999' )
attributeTypes: ( whenCreated-OID NAME 'whenCreated' DESC 'The date when this object was created. This value is replicated and is in the global catalog.' SYNTAX 1.3.6.1.4.1.1466.115.121.1.24 SINGLE-VALUE USAGE userApplications )
attributeTypes: ( SAMAccountName-OID NAME 'sAMAccountName' DESC 'The logon name used to support clients and servers running earlier versions of the operating system, such as Windows NT 4.0, Windows 95, Windows 98, and LAN Manager.' SINGLE-VALUE USAGE userApplications X-MAX-VALUE-LENGTH '20' )
attributeTypes: ( logonCount-OID NAME 'logonCount' DESC 'The number of times the account has successfully logged on. A value of 0 indicates that the value is unknown.' SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 SINGLE-VALUE USAGE userApplications )
attributeTypes: ( userAccountControl-OID NAME 'userAccountControl' DESC 'Flags that control the behavior of the user account.' SINGLE-VALUE USAGE userApplications X-MIN-INT-VALUE '0' X-MAX-INT-VALUE '9999' )
attributeTypes: ( primaryGroupID-OID NAME 'primaryGroupID' DESC 'Contains the relative identifier (RID) for the primary group of the user. By default, this is the RID for the Domain Users group.' SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 SINGLE-VALUE USAGE userApplications X-MIN-INT-VALUE '0' X-MAX-INT-VALUE '9999' )
attributeTypes: ( codePage-OID NAME 'codePage' DESC 'Specifies the code page for the user\27s language of choice. This value is not used by Windows 2000.' SINGLE-VALUE USAGE userApplications )
attributeTypes: ( lastLogon-OID NAME 'lastLogon' DESC 'The last time the user logged on. This value is stored as a large integer that represents the number of 100-nanosecond intervals since January 1, 1601 (UTC). A value of zero means that the last logon time is unknown.' SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 SINGLE-VALUE USAGE userApplications )
attributeTypes: ( uSNCreated-OID NAME 'uSNCreated' DESC 'The update sequence number (USN) assigned at object creation.' SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 SINGLE-VALUE USAGE userApplications )
attributeTypes: ( SAMAccountType-OID NAME 'sAMAccountType' DESC 'This attribute contains information about every account type object. You can enumerate a list of account types or you can use the Display Information API to create a list. Because computers, normal user accounts, and trust accounts can also be enumerated as user objects, the values for these accounts must be a contiguous range.' SINGLE-VALUE USAGE userApplications )
attributeTypes: ( objectSid-OID NAME 'objectSid' DESC 'A binary value that specifies the security identifier (SID) of the user. The SID is a unique value used to identify the user as a security principal.' SYNTAX 1.3.6.1.4.1.1466.115.121.1.40 SINGLE-VALUE USAGE userApplications )
attributeTypes: ( accountExpires-OID NAME 'accountExpires' DESC 'The date when the account expires. This value represents the number of 100-nanosecond intervals since January 1, 1601 (UTC). A value of 0 or 0x7FFFFFFFFFFFFFFF (9223372036854775807) indicates that the account never expires.' SINGLE-VALUE USAGE userApplications )
objectClasses: ( user-OID NAME 'user' DESC 'This class is used to store information about an employee or contractor who works for an organization. It is also possible to apply this class to long term visitors.' SUP inetOrgPerson STRUCTURAL MUST sAMAccountName MAY ( accountExpires $ badPasswordTime $ badPwdCount $ codePage $ company $ countryCode $ distinguishedName $ lastLogoff $ lastLogon $ logonCount $ name $ objectGUID $ objectSid $ primaryGroupID $ pwdLastSet $ sAMAccountType $ uSNChanged $ uSNCreated $ userAccountControl $ whenChanged $ whenCreated ) )

